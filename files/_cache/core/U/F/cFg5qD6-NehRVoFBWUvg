      <media:content url="https://imagenes.elpais.com/resizer/v2/YKBSJY5MVFFZBM4CHPNRD4UM7A.jpg?auth=f66df115b8786411644714a31753510a6f4332a3f017913d6c1c003eb469fef5" type="image/jpeg" medium="image">
