      <media:content url="https://imagenes.elpais.com/resizer/v2/KGA3YCICURG2LMJUASI7AHY3KQ.jpg?auth=01251927820041b104cab66f1cf388d3a1ec03cfbd32df4ab00d9da7e8ab38a5" type="image/jpeg" medium="image">
