        <media:content url="https://imagenes.elpais.com/resizer/v2/XHM7NCGSZFDRHENHZYUN2ENROY.jpg?auth=84d508e80116c059a1e4663de824c41da20d9e43ff2d2146c68c583e53f2f354" type="image/jpeg" medium="image">
